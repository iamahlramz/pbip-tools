<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <rd:ReportUnitType>Inch</rd:ReportUnitType>
  <DataSources>
    <DataSource Name="AdventureWorksDS">
      <ConnectionProperties>
        <DataProvider>PBIDATASET</DataProvider>
        <ConnectString>Data Source=localhost;Initial Catalog=AdventureWorks</ConnectString>
      </ConnectionProperties>
    </DataSource>
    <DataSource Name="SecondaryDS">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=localhost;Initial Catalog=Staging</ConnectString>
      </ConnectionProperties>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="SalesByYear">
      <Query>
        <DataSourceName>AdventureWorksDS</DataSourceName>
        <CommandText><![CDATA[EVALUATE
SUMMARIZECOLUMNS(
    'DimDate'[CalendarYear],
    'DimDate'[MonthName],
    "TotalSales", [Total Sales],
    "TotalQuantity", [Total Quantity],
    "AvgPrice", [Average Price]
)]]></CommandText>
      </Query>
      <Fields>
        <Field Name="CalendarYear">
          <DataField>CalendarYear</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="MonthName">
          <DataField>MonthName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalSales">
          <DataField>TotalSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalQuantity">
          <DataField>TotalQuantity</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="AvgPrice">
          <DataField>AvgPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CustomerSummary">
      <Query>
        <DataSourceName>AdventureWorksDS</DataSourceName>
        <CommandText><![CDATA[EVALUATE
SUMMARIZECOLUMNS(
    'DimCustomer'[CustomerName],
    "CustomerSales", [Total Sales]
)]]></CommandText>
      </Query>
      <Fields>
        <Field Name="CustomerName">
          <DataField>CustomerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustomerSales">
          <DataField>CustomerSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="TopProducts">
      <Query>
        <DataSourceName>AdventureWorksDS</DataSourceName>
        <CommandText><![CDATA[EVALUATE
TOPN(
    10,
    SUMMARIZECOLUMNS(
        'FactSales'[ProductName],
        "ProductSales", [Total Sales]
    ),
    [ProductSales], DESC
)]]></CommandText>
      </Query>
      <Fields>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductSales">
          <DataField>ProductSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="Year">
      <DataType>Integer</DataType>
      <Prompt>Select Year</Prompt>
      <DefaultValue>
        <Values>
          <Value>2024</Value>
        </Values>
      </DefaultValue>
    </ReportParameter>
    <ReportParameter Name="Region">
      <DataType>String</DataType>
      <Prompt>Select Region</Prompt>
      <AllowBlank>true</AllowBlank>
      <DefaultValue>
        <Values>
          <Value>All</Value>
        </Values>
      </DefaultValue>
      <ValidValues>
        <ParameterValues>
          <ParameterValue>
            <Value>All</Value>
            <Label>All Regions</Label>
          </ParameterValue>
          <ParameterValue>
            <Value>North</Value>
            <Label>North</Label>
          </ParameterValue>
          <ParameterValue>
            <Value>South</Value>
            <Label>South</Label>
          </ParameterValue>
        </ParameterValues>
      </ValidValues>
    </ReportParameter>
  </ReportParameters>
  <ReportSections>
    <ReportSection>
      <Body>
        <ReportItems>
          <Textbox Name="ReportTitle">
            <CanGrow>true</CanGrow>
            <KeepTogether>true</KeepTogether>
            <Paragraphs>
              <Paragraph>
                <TextRuns>
                  <TextRun>
                    <Value>Adventure Works Sales Report</Value>
                    <Style>
                      <FontFamily>Segoe UI</FontFamily>
                      <FontSize>20pt</FontSize>
                      <FontWeight>Bold</FontWeight>
                      <Color>#80004B</Color>
                    </Style>
                  </TextRun>
                </TextRuns>
              </Paragraph>
            </Paragraphs>
            <Top>0in</Top>
            <Left>0in</Left>
            <Height>0.6in</Height>
            <Width>7.5in</Width>
          </Textbox>
          <Tablix Name="SalesTable">
            <DataSetName>SalesByYear</DataSetName>
            <TablixBody>
              <TablixColumns>
                <TablixColumn>
                  <Width>1.5in</Width>
                </TablixColumn>
                <TablixColumn>
                  <Width>1.5in</Width>
                </TablixColumn>
                <TablixColumn>
                  <Width>2in</Width>
                </TablixColumn>
              </TablixColumns>
              <TablixRows>
                <TablixRow>
                  <Height>0.25in</Height>
                  <TablixCells>
                    <TablixCell>
                      <CellContents>
                        <Textbox Name="HeaderYear">
                          <Paragraphs>
                            <Paragraph>
                              <TextRuns>
                                <TextRun>
                                  <Value>Year</Value>
                                  <Style>
                                    <FontWeight>Bold</FontWeight>
                                  </Style>
                                </TextRun>
                              </TextRuns>
                            </Paragraph>
                          </Paragraphs>
                        </Textbox>
                      </CellContents>
                    </TablixCell>
                    <TablixCell>
                      <CellContents>
                        <Textbox Name="HeaderMonth">
                          <Paragraphs>
                            <Paragraph>
                              <TextRuns>
                                <TextRun>
                                  <Value>Month</Value>
                                  <Style>
                                    <FontWeight>Bold</FontWeight>
                                  </Style>
                                </TextRun>
                              </TextRuns>
                            </Paragraph>
                          </Paragraphs>
                        </Textbox>
                      </CellContents>
                    </TablixCell>
                    <TablixCell>
                      <CellContents>
                        <Textbox Name="HeaderSales">
                          <Paragraphs>
                            <Paragraph>
                              <TextRuns>
                                <TextRun>
                                  <Value>Total Sales</Value>
                                  <Style>
                                    <FontWeight>Bold</FontWeight>
                                  </Style>
                                </TextRun>
                              </TextRuns>
                            </Paragraph>
                          </Paragraphs>
                        </Textbox>
                      </CellContents>
                    </TablixCell>
                  </TablixCells>
                </TablixRow>
                <TablixRow>
                  <Height>0.25in</Height>
                  <TablixCells>
                    <TablixCell>
                      <CellContents>
                        <Textbox Name="CellYear">
                          <Paragraphs>
                            <Paragraph>
                              <TextRuns>
                                <TextRun>
                                  <Value>=Fields!CalendarYear.Value</Value>
                                </TextRun>
                              </TextRuns>
                            </Paragraph>
                          </Paragraphs>
                        </Textbox>
                      </CellContents>
                    </TablixCell>
                    <TablixCell>
                      <CellContents>
                        <Textbox Name="CellMonth">
                          <Paragraphs>
                            <Paragraph>
                              <TextRuns>
                                <TextRun>
                                  <Value>=Fields!MonthName.Value</Value>
                                </TextRun>
                              </TextRuns>
                            </Paragraph>
                          </Paragraphs>
                        </Textbox>
                      </CellContents>
                    </TablixCell>
                    <TablixCell>
                      <CellContents>
                        <Textbox Name="CellSales">
                          <Paragraphs>
                            <Paragraph>
                              <TextRuns>
                                <TextRun>
                                  <Value>=Fields!TotalSales.Value</Value>
                                  <Style>
                                    <Format>#,##0.00</Format>
                                  </Style>
                                </TextRun>
                              </TextRuns>
                            </Paragraph>
                          </Paragraphs>
                        </Textbox>
                      </CellContents>
                    </TablixCell>
                  </TablixCells>
                </TablixRow>
              </TablixRows>
            </TablixBody>
            <TablixColumnHierarchy>
              <TablixMembers>
                <TablixMember/>
                <TablixMember/>
                <TablixMember/>
              </TablixMembers>
            </TablixColumnHierarchy>
            <TablixRowHierarchy>
              <TablixMembers>
                <TablixMember>
                  <KeepWithGroup>After</KeepWithGroup>
                </TablixMember>
                <TablixMember>
                  <Group Name="DetailGroup"/>
                </TablixMember>
              </TablixMembers>
            </TablixRowHierarchy>
            <Top>0.8in</Top>
            <Left>0in</Left>
            <Height>0.5in</Height>
            <Width>5in</Width>
          </Tablix>
          <Chart Name="SalesChart">
            <ChartCategoryHierarchy>
              <ChartMembers>
                <ChartMember>
                  <Group Name="ChartCatGroup">
                    <GroupExpressions>
                      <GroupExpression>=Fields!CalendarYear.Value</GroupExpression>
                    </GroupExpressions>
                  </Group>
                </ChartMember>
              </ChartMembers>
            </ChartCategoryHierarchy>
            <ChartSeriesHierarchy>
              <ChartMembers>
                <ChartMember>
                  <Label>Total Sales</Label>
                </ChartMember>
              </ChartMembers>
            </ChartSeriesHierarchy>
            <ChartData>
              <ChartSeriesCollection>
                <ChartSeries Name="Sales">
                  <ChartDataPoints>
                    <ChartDataPoint>
                      <ChartDataPointValues>
                        <Y>=Sum(Fields!TotalSales.Value)</Y>
                      </ChartDataPointValues>
                    </ChartDataPoint>
                  </ChartDataPoints>
                </ChartSeries>
              </ChartSeriesCollection>
            </ChartData>
            <DataSetName>SalesByYear</DataSetName>
            <Top>1.5in</Top>
            <Left>0in</Left>
            <Height>3in</Height>
            <Width>7.5in</Width>
          </Chart>
        </ReportItems>
        <Height>5in</Height>
      </Body>
      <Width>7.5in</Width>
      <Page>
        <PageHeader>
          <Height>0.5in</Height>
          <PrintOnFirstPage>true</PrintOnFirstPage>
          <PrintOnLastPage>true</PrintOnLastPage>
          <ReportItems>
            <Textbox Name="PageHeaderText">
              <Paragraphs>
                <Paragraph>
                  <TextRuns>
                    <TextRun>
                      <Value>Adventure Works - Confidential</Value>
                      <Style>
                        <FontSize>8pt</FontSize>
                        <FontStyle>Italic</FontStyle>
                        <Color>#666666</Color>
                      </Style>
                    </TextRun>
                  </TextRuns>
                </Paragraph>
              </Paragraphs>
              <Height>0.3in</Height>
              <Width>7.5in</Width>
            </Textbox>
          </ReportItems>
        </PageHeader>
        <PageFooter>
          <Height>0.5in</Height>
          <PrintOnFirstPage>true</PrintOnFirstPage>
          <PrintOnLastPage>true</PrintOnLastPage>
          <ReportItems>
            <Textbox Name="PageNumber">
              <Paragraphs>
                <Paragraph>
                  <TextRuns>
                    <TextRun>
                      <Value>=Globals!PageNumber &amp; " of " &amp; Globals!TotalPages</Value>
                      <Style>
                        <FontSize>8pt</FontSize>
                      </Style>
                    </TextRun>
                  </TextRuns>
                </Paragraph>
              </Paragraphs>
              <Height>0.3in</Height>
              <Width>7.5in</Width>
              <Style>
                <TextAlign>Right</TextAlign>
              </Style>
            </Textbox>
          </ReportItems>
        </PageFooter>
        <PageHeight>11in</PageHeight>
        <PageWidth>8.5in</PageWidth>
        <LeftMargin>0.5in</LeftMargin>
        <RightMargin>0.5in</RightMargin>
        <TopMargin>0.5in</TopMargin>
        <BottomMargin>0.5in</BottomMargin>
      </Page>
    </ReportSection>
  </ReportSections>
</Report>
