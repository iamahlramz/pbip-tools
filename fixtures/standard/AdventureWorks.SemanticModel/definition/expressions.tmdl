expression fnGetLatestFile =
		let
			Source = (folderPath as text, filePrefix as text) =>
				let
					Files = Folder.Files(folderPath),
					Filtered = Table.SelectRows(Files, each Text.StartsWith([Name], filePrefix)),
					Sorted = Table.Sort(Filtered, {{"Date modified", Order.Descending}}),
					Latest = Sorted{0}[Content]
				in
					Latest
		in
			Source
	lineageTag: 70000000-0000-0000-0000-000000000002
	queryGroup: Functions

