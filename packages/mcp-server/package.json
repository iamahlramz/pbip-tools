{
  "name": "@pbip-tools/mcp-server",
  "version": "0.1.0",
  "description": "MCP server exposing Power BI PBIP project tools for AI assistants",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "pbip-mcp": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@pbip-tools/core": "0.1.0",
    "@pbip-tools/tmdl-parser": "0.1.0",
    "@pbip-tools/visual-handler": "0.1.0",
    "@pbip-tools/project-discovery": "0.1.0",
    "@pbip-tools/dax-formatter": "0.1.0",
    "@modelcontextprotocol/sdk": "^1.27.0",
    "zod": "^3.25.0"
  },
  "license": "MIT"
}
