{
  "name": "@pbip-tools/core",
  "version": "0.2.0",
  "description": "Core types and constants for pbip-tools",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
